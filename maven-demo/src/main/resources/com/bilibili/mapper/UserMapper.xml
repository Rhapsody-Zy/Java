<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bilibili.mapper.UserMapper">
    <select id="selectAll" resultType="com.bilibili.pojo.User">
        select * from tb_user;
    </select>
    <select id="selectById" resultType="com.bilibili.pojo.User">
        select tb_user where id = #{id}

    </select>
</mapper>